<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="user" class="by.galov.organizer.beans.User"></bean>
	<bean id="task" class="by.galov.organizer.beans.Task"></bean>
	<bean id="userfile" class="by.galov.organizer.beans.UserFile">
	   <constructor-arg ref="user"/>
	   <constructor-arg ref="task"/>
	   
	   <property name="fileName" value="asdf"/>
	</bean>
</beans>
